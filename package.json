{
	"name": "rrdbg",
	"displayName": "rrdbg",
	"description": "rr debugger extension for VS Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rrdbg.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rrdbg.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@babel/cli": "^7.16.0",
		"@babel/core": "^7.16.0",
		"@babel/eslint-parser": "^7.16.0",
		"@babel/preset-env": "^7.16.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.61.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4"
	},
	"author": {
		"name": "Andreas Farre",
		"email": "andreas@farre.se"
	},
	"maintainers": [
		{
			"name": "Andreas Farre",
			"email": "andreas@farre.se"
		},
		{
			"name": "Simon Farre",
			"email": "simon.farre.cx@gmail.com"
		}
	],
	"dependencies": {
		"gdb-js": "^1.3.0"
	}
}
